BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//birthdayfeed.subtlecoolness.com
CALSCALE:GREGORIAN
X-WR-CALNAME:birthdayfeed
X-WR-TIMEZONE:UTC
X-WR-CALDESC:Birthday calendar provided by 
 http://birthdayfeed.subtlecoolness.com/
{% for birthday in birthdays %}BEGIN:VEVENT
DTSTART;VALUE=DATE:{{ birthday.dtstart }}
DTEND;VALUE=DATE:{{ birthday.dtend }}
DTSTAMP:{{ dtstamp }}
UID:{{ birthday.uid }}@
 birthdayfeed.subtlecoolness.com
CREATED:{{ dtstamp }}
DESCRIPTION:{{ birthday.description }}
LAST-MODIFIED:{{ dtstamp }}
SUMMARY:{{ birthday.summary }}
TRANSP:TRANSPARENT
END:VEVENT
{% endfor %}END:VCALENDAR